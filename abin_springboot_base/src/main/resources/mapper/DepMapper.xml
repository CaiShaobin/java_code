<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.abin.dao.DepDao">


    <select id="getDepList" resultType="com.abin.pojo.Department">
        select * from department;
    </select>
    <select id="getDepById" resultType="com.abin.pojo.Department">
        select * from department where depId = #{id}
    </select>
</mapper>